{
  "name": "react-mobx-boilerplate",
  "version": "2.3.0",
  "description": "React MObx Boilerplate allows you to start new web project from scratch using Mobx-State-Tree, Mobx, React and Styled components.",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server  --config config/webpack.config.dev.js --mode development",
    "bld": "sh -ac '. ./.env.${REACT_APP_BUILD_MODE}; node ./config/scripts/build.js'",
    "build": "REACT_APP_BUILD_MODE=development yarn run bld",
    "staging": "REACT_APP_BUILD_MODE=staging yarn run bld",
    "production": "REACT_APP_BUILD_MODE=production yarn run bld"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VladMorzhanov/mst-react-boilerplate"
  },
  "keywords": [
    "mobx-state-tree",
    "react",
    "reactjs",
    "boilerplate",
    "mobx"
  ],
  "author": "Vlad Morzhanov",
  "license": "MIT",
